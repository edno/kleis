{
    "name": "edno/kleis",
    "description": "Squid Guard Basic Front End",
    "keywords": ["users", "management"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/framework": "~5.7",
        "whitehat101/apr1-md5": "~1.0"
    },
    "require-dev": {
        "codeception/codeception": "2.5.4",
        "site5/phantoman": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "database",
            "tests/_data"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "build-archive": [
            "composer archive --format=zip --dir=bin/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "process-timeout": 0
    },
    "archive": {
        "exclude": [
            "bin",
            "tests",
            "node_modules",
            "storage/*",
            "*file",
            "*.xml",
            "*.yml",
            "*.js",
            "*.json",
            "!composer.json",
            "server.php",
            "vendor"
        ]
    }
}
